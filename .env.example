# Variables de entorno para Postgres
POSTGRES_USER="your_postgres_user"
POSTGRES_PASSWORD="your_secure_password"
POSTGRES_SERVER="postgres_service"
POSTGRES_PORT="5432"
POSTGRES_DB="translator_db"

# Variables de entorno para SQL Server
SQL_SERVER_PASSWORD="YourSecure@Password123"

# Variables de entorno para Neo4j
NEO4J_PASSWORD="your_neo4j_password"

# Variables de entorno para JWT
# SECRET_KEY: Clave secreta para firmar tokens JWT (usar un valor seguro de 32+ caracteres)
SECRET_KEY="your-super-secret-key-change-in-production"
# ALGORITHM: Algoritmo de firma JWT (HS256 recomendado)
ALGORITHM="HS256"
# ACCESS_TOKEN_EXPIRE_MINUTES: Tiempo de expiración del token en minutos (1440 = 24 horas según HU AUM-01)
ACCESS_TOKEN_EXPIRE_MINUTES="1440"

# Variables de entorno para CORS
BACKEND_CORS_ORIGINS="http://localhost:3000"

# Variables de entorno para ANTLR
# ANTLR_TIMEOUT_MS=""